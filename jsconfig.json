{
    "compilerOptions": {
      "target": "es6",
      "module": "esnext",
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "baseUrl": "./src/renderer/src",
      "paths": {
        "@/*": ["*"]
      },
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "out", "dist"]
  }
  